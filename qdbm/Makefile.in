crtest : crtest.o $(MYLIBS)
	$(LDENV) $(CC) $(CFLAGS) -o $@ crtest.o $(LDFLAGS)


crtsv : crtsv.o $(MYLIBS)
	$(LDENV) $(CC) $(CFLAGS) -o $@ crtsv.o $(LDFLAGS)


rlmgr : rlmgr.o $(MYLIBS)
