rltest : rltest.o $(MYLIBS)
	$(LDENV) $(CC) $(CFLAGS) -o $@ rltest.o $(LDFLAGS)


hvmgr : hvmgr.o $(MYLIBS)
	$(LDENV) $(CC) $(CFLAGS) -o $@ hvmgr.o $(LDFLAGS)


hvtest : hvtest.o $(MYLIBS)
